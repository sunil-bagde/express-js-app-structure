{
  "name": "node-project-structure-express",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "start": "node build/app.js",
    "build": "tsc -p ./",
    "lint": "eslint \"app/**/*.{js,ts}\" --quiet --ignore-path .gitignore ",
    "lint:fix": "yarn lint -- --fix",
    "postinstall": "link-module-alias",
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage ",
    "test:coverage": "jest --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "module-alias": "^2.2.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest-junit": "^16.0.0",
    "link-module-alias": "^1.2.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "*"
  },
  "author": "Sunil Bagde <sunilbagde>",
  "_moduleAliases": {
    "~": "./build"
  },
  "imports": {
    "#lib/*": "./build/lib/*.js"
  }
}
